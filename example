local module = loadstring(game:HttpGet("https://raw.githubusercontent.com/kaiforreal/replobui/refs/heads/main/replobui.source"))()
module:checkplayer()
module:theme("black") --more colors if u want, your choices are: ("red", "blue", "green", "purple")
local main = module:main("FUCKK YEAAAS", "DESCRIPTION ACTUALLY WORKS NOW LETS FUCKING GOOOOOOOOOOOOOOOOO")

module:CreateButton("press me boy.", "main", function()
	print("Clicked main!")
end)

module:CreateButton("press him NOW!!", "main", function()
	print("Clicked!")
end)

module:CreateButton("yeah cmon dude", "sidebar", function()
	print("Clicked sidebar!")
end)

local subGui = module:createsubgui()
module:CreateToggle("Sub Toggle", subGui, true, function(state)
	print("Sub toggle:", state)
end)
