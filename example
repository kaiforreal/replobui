local module = loadstring(game:HttpGet("https://raw.githubusercontent.com/kaiforreal/replobui/refs/heads/main/replobui.source"))()
module:checkplayer()
module:theme("black")

local main = module:main("MAIN GUI FAWK", "Description (probably dont work yet LMAOOO)")

module:CreateButton("touch me boy.", main, function()
	print("Button touched")
end)

module:CreateToggle("God Mode", main, false, function(state)
	print("Toggle is now:", state)
end)

module:CreateToggle("left bar side thing idk anymore", sidebar, false, function(state)
  print("boyy we got a toggle on our sidebar we're COOKING!!", state)
end)

module:CreateSlider("Volume", "Adjust volume", main, 0, 100)

local subGui = module:createsubgui()
module:CreateToggle("Sub Toggle", subGui, true, function(state)
	print("Sub Toggle:", state)
end)
module:CreateSlider("Sub Slider", "Sub desc", subGui, 0, 50)
